{
    "name": "Neut",
    "description": "Neut agent for Chrome browser",
    "version": "0.1",
    "default_locale": "en",
    "icons": {
        "128": "img/neut.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["src/url.js", "src/navigation_collector.js", "src/background.js"]
    },
    "browser_action": {
        "default_icon": "img/neut.png",
        "default_popup": "src/popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "webNavigation"
    ],
    "manifest_version": 2
}
